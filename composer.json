{
    "name": "jeroenvanderlaan/php-types",
    "description": "Utilities for working with and formatting PHP types.",
    "keywords": ["type", "types", "datatype", "dump", "inspect"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jeroen van der Laan",
            "email": "jeroenvanderlaan91@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Jeroenvanderlaan\\Types\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jeroenvanderlaan\\Types\\Tests\\": "tests/"
        }
    }
}
